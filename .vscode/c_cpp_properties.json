{
    "configurations": [
        {
            "name": "Linux",
            "compileCommands": [
                "${workspaceFolder}/build/compile_commands.json"
            ],
            "configurationProvider": "ms-vscode.cmake-tools",
            "intelliSenseMode": "linux-clang-x64",
            "cStandard": "c23",
            "cppStandard": "c++23",
            "includePath": [
                "${workspaceFolder}/include",
                "${workspaceFolder}/src",
